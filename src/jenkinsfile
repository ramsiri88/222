pipeline {
    agent any

    stages {
        stage('compile stage') {
            steps {
                withmaven(maven: apache-maven-3.5.3){
                  sh 'maven clean compile'
                  }
                 }
                }
                  
        stage('Testing stage') {
            steps {
                withmaven(maven: apache-maven-3.5.3){
                 sh 'maven test'
             }
            }
        }
    }
}
